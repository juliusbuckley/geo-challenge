{
  "name": "geo-location",
  "version": "1.0.0",
  "description": "react boilerplate",
  "main": "app.js",
  "scripts": {
    "start": "npm run prod:buildserver; npm run webpack & node ./src/server-dist/app.js",
    "prod:buildserver": "babel ./src/server -d ./src/server-dist --presets es2015 --copy-files",
    "dev:start": "npm run webpack & nodemon ./src/server/app.js --exec babel-node --presets es2015",
    "webpack": "./node_modules/.bin/webpack -d --watch",
    "test": "./node_modules/mocha/bin/mocha ./src/server-dist/test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juliusbuckley/geo-location.git"
