  "bugs": {
    "url": "https://github.com/juliusbuckley/geo-location/issues"
  },
  "homepage": "https://github.com/juliusbuckley/geo-location#readme",
  "dependencies": {
    "axios": "^0.15.3",
    "body-parser": "^1.16.0",
    "bootstrap": "^3.3.7",
    "express": "^4.14.0",
    "fast-csv": "^2.3.1",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
